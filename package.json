{
  "name": "koval-ai-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Koval AI - AI-powered dive log and underwater analysis platform",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "turbo run build --filter=!@koval-ai/mobile",
    "build:all": "turbo run build",
    "dev": "turbo run dev --filter=!@koval-ai/mobile",
    "dev:web": "turbo run dev --filter=@koval-ai/web",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:upload": "node test-unified-upload.js",
    "test:upload:prod": "node test-unified-upload.js --production",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "web": "turbo run dev --filter=@koval-ai/web",
    "mobile": "turbo run dev --filter=@koval-ai/mobile",
    "core": "turbo run build --filter=@koval-ai/core",
    "format": "prettier --write .",
    "start:ts": "ts-node -r tsconfig-paths/register scripts/ingest-documents.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:setup": "node scripts/setup-storage.js",
    "supabase:status": "supabase status",
    "db:migrate": "supabase db push",
    "db:generate-types": "supabase gen types typescript --local > types/supabase.ts",
    "ingest-knowledge": "node scripts/auto-ingest-knowledge.js",
    "ingest-knowledge:watch": "node scripts/auto-ingest-knowledge.js --watch",
    "ingest-knowledge:file": "node scripts/auto-ingest-knowledge.js --file",
    "update-pinecone": "npm run ingest-knowledge"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@pinecone-database/pinecone": "^6.1.2",
    "@supabase/supabase-js": "^2.55.0",
    "axios": "^1.10.0",
    "browser-image-compression": "^2.0.2",
    "chokidar": "^4.0.3",
    "critters": "^0.0.23",
    "dotenv": "^17.2.1",
    "form-data": "^4.0.4",
    "formidable": "^3.5.4",
    "gpt-3-encoder": "^1.1.4",
    "gray-matter": "^4.0.3",
    "multer": "^2.0.2",
    "next": "^14.2.5",
    "next-connect": "^1.0.0",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0",
    "vercel": "^32.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.1",
    "@types/express": "^5.0.3",
    "@types/formidable": "^3.4.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.40",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.10.0",
    "turbo": "^1.10.12",
    "typescript": "^4.9.5",
    "webpack": "^5.100.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "optionalDependencies": {
    "sharp": "^0.33.5"
  }
}
