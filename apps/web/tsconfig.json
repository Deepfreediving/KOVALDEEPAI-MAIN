{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "esnext",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@types/*": [
        "./types/*"
      ]
    },
    "types": [
      "node"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "next-env.d.ts",
    "types/**/*.d.ts",
    ".next/types/**/*.ts",
    "next.config.js",
    "vercel.json"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "out",
    "../mobile",
    "../../apps/mobile",
    "../../packages/mobile",
    "**/react-native",
    "**/react-native-*",
    "public/wix-widget-loader-v3.js",
    "**/wix-widget-loader*.js",
    "wix-site/**",
    "Wix App/**",
    "wix page/**",
    "wix guides/**"
  ]
}
