# Development Environment Variables Template
# Copy this file to .env.development and fill in your actual API keys
# Never commit .env.development with real secrets

# OpenAI API settings
OPENAI_MODEL=gpt-4
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY_HERE
OPENAI_API_URL=https://api.openai.com/v1
OPENAI_ORG=org-YOUR_OPENAI_ORG_ID_HERE
OPENAI_API_BASE=us-east-1
OPENAI_ASSISTANT_ID=asst_YOUR_ASSISTANT_ID_HERE
OPENAI_PROJECT_ID=proj_YOUR_PROJECT_ID_HERE

# Pinecone API settings
PINECONE_API_KEY=pcsk_YOUR_PINECONE_API_KEY_HERE
PINECONE_HOST=https://your-index-name.svc.environment.pinecone.io
PINECONE_INDEX=your-index-name
PINECONE_ENVIRONMENT=us-east-1

